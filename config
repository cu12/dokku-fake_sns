#!/usr/bin/env bash
export FAKESNS_IMAGE=${FAKESNS_IMAGE:="seayou/fake-sns"}
export FAKESNS_IMAGE_VERSION=${FAKESNS_IMAGE_VERSION:="latest"}
export FAKESNS_ROOT=${FAKESNS_ROOT:="/var/lib/dokku/services/fakesns"}

export PLUGIN_COMMAND_PREFIX="sns"
export PLUGIN_DATA_ROOT=$FAKESNS_ROOT
export PLUGIN_DATASTORE_PORTS=(9292)
export PLUGIN_DEFAULT_ALIAS="FAKESNS"
export PLUGIN_ALT_ALIAS="DOKKU_FAKESNS"
export PLUGIN_IMAGE=$FAKESNS_IMAGE
export PLUGIN_IMAGE_VERSION=$FAKESNS_IMAGE_VERSION
export PLUGIN_SCHEME="http"
export PLUGIN_SERVICE="Fakesns"
